                                    <option value="host">Host</option>
                                    <option value="instant">Instant</option>
                                    <option value="land">Land</option>
                                </select>
                            </section>
                            <label for="mana-symbol" class="col-12 ch1 text-center ctext-white fs-3">Soort mana</label>
