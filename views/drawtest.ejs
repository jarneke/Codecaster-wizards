                <h1 class="ch1 fs-2 ctext-secondary">Getrokken kaarten</h1>
                <button class="btn-close text-reset bg-danger" type="button" data-bs-toggle="offcanvas" aria-label="close">
                </button>
            </section>
            <section class="offcanvas-body">
                <section>
