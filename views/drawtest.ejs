<%- include("partials/header") %>
    <main class="d-flex flex-wrap align-items-center justify-content-center ctext-white">
        <section class="d-flex flex-wrap col-12 col-md-10 col-lg-8 m-3">
            <form action="" class="col-12 col-sm-4 pt-2 p-2">
                <select name="decks" id="decks" class="form-select p-2">
                    <option value=" deck1">Deck 1</option>
                    <option value="deck2">Deck 2</option>
                    <option value="deck3">Deck 3</option>
                    <option value="deck4">Deck 4</option>
                    <option value="deck5">Deck 5</option>
                    <option value="deck6">Deck 6</option>
                </select>
            </form>
            <form action="" class="col-12 col-sm-4 p-2">
                <section class="form-control d-flex p-1">
                    <button class="bg-white text-secondary p-1 border-0" type="submit" id="search">
                        <svg height="1rem" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">
                            <!--!Font Awesome Free 6.5.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.-->
                            <path d="M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352a144 144 0 1 0 0-288 144 144 0 1 0 0 288z" />
                        </svg>
                    </button>
                    <input aria-label="Search" placeholder="Zoeken..." type="search" name="cardLookup" id="cardLookup" class="form-control border-0 p-0" />
                </section>
            </form>
            <section class="col-12 col-sm-4 p-2">
                <a class="btn btn-dark col-12 p-2" data-bs-toggle="offcanvas" href="#pulledCards" role="button" aria-controls="pulledCards">
                    Getrokken kaarten
                </a>
            </section>
        </section>
        <section class="d-flex justify-content-center flex-wrap mb-3 col-12 col-sm-8 col-md-4">
            <img src="assets/images/demoCards/MTG-card_back.jpg" class="col-10">
            <p class="col-12 text-center ch1 mt-2 fs-2">56/60</p>
        </section>
        <section class="d-flex flex-wrap justify-content-center p-3 text-center col-12 col-md-4">
            <article class="col-12 mb-5">
                <h3>Kans om kaart te trekken 3.33%</h3>
                <p>Deze kaart zit 2 keer in het deck</p>
            </article>
            <button class="btn btn-dark mb-3 col-7">Trek een kaart</button>
            <button class="btn btn-danger col-7">Reset</button>
        </section>
        <section class=" d-flex justify-content-center mt-3 mb-3 col-12 col-sm-8 col-md-4">
            <button class="btn d-flex justify-content-center" data-bs-toggle="offcanvas" href="#pulledCards" role="button" aria-controls="pulledCards">
                <img src="assets/images/demoCards/card4.jpg" class="col-10">
            </button>
        </section>
